- Name: Reproduce_Stock

  clear previous: !!python/bool True

  Model:
   type: SymbolicBalanceBoardModel
   initial state: [5, 0, 0, 0, 0, 0]
   board centre of gravity x-direction: -0.003
   board centre of gravity y-direction: 0.01726
   board inertia: 0.00115
   board mass: 1.0149
   carriage mass: 0.4075
   carriage centre of gravity y-direction: 0.02574
   carriage inertia: 0.000126
   cylinder mass: 3.9901
   carriage centre of gravity y-direction: -0.0009
   carriage inertia: 0.00847

  Solver:
   type: ODEInt
   end time: 5

  Controller:
   type: BBLinearController
   poles: [-4,-4,-5.7,-3.03,-3.3,-1]
   steady_theta: 0
   board centre of gravity x-direction: -0.003
   board centre of gravity y-direction: 0.01726
   board inertia: 0.00115
   board mass: 1.0149
   carriage mass: 0.4075
   carriage centre of gravity y-direction: 0.02574
   carriage inertia: 0.000126
   cylinder mass: 3.9901
   carriage centre of gravity y-direction: -0.0009
   carriage inertia: 0.00847

  ModelMixer:
   type: AdditiveMixer
   Input A: Controller

